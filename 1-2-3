1.

SELECT
     zip,
	 MAX(maxtemperaturef) m_temp
FROM
     weather
GROUP BY 1
ORDER BY maxtemperaturef
LIMIT 1

2.

SELECT
     start_station,
	 COUNT(*)
FROM
     trips
GROUP BY 1

3.

SELECT    
	 duration,
	 MIN(duration) shortest_trip 
FROM 
     trips
GROUP BY 1
ORDER BY duration 
LIMIT 1

4.

SELECT 
     end_station,
	 AVG(duration) average_trip
FROM 
     trips
GROUP BY end_station
ORDER BY average_trip